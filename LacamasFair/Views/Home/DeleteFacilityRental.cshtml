@model FacilityRental

@{
    ViewData["Title"] = "DeleteFacilityRental";
}

<h1>Delete @Model.FacilityName</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.FacilityName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FacilityName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Price)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Price)
        </dd>
    </dl>
    
    <form asp-action="DeleteFacilityRental">
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="FacilityRental">Back to Facility Rental</a>
    </form>
</div>
